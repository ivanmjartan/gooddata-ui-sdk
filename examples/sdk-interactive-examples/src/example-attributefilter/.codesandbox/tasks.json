{
    "$schema": "https://codesandbox.io/schemas/tasks.json",
    "tasks": {
      "start": {
        "name": "Start Dev Server",
        "command": "npm start",
        // With the `preview` field set, the GitHub App will also add a link
        // to that preview to PRs, and it will become part of a GitHub
        // deployment:
        "preview": {
          "port": 8080,
          "prLink": "direct"
        }
      }
    }
  }